{
  "env": "NODE_ENV",
  "port": "PORT",
  "rateLimit": {
    "enabled": "RATE_LIMIT_ENABLED",
    "max": "RATE_LIMIT_MAX",
    "timeWindow": "RATE_LIMIT_TIME_WINDOW"
  },
  "adminKey": "ADMIN_KEY",
  "cloudinary": {
    "cloudName": "CLOUDINARY_CLOUD_NAME",
    "apiKey": "CLOUDINARY_API_KEY",
    "apiSecret": "CLOUDINARY_API_SECRET"
  },
  "db": {
    "url": "DB_URL",
    "ssl": "DB_SSL"
  },
  "google": {
    "clientId": "GOOGLE_CLIENT_ID",
    "serviceAccountKeyUrl": "SERVICE_ACCOUNT_KEY_URL",
    "databaseURL": "FIREBASE_DATABASE_URL"
  },
  "redis": {
    "url": "REDIS_URL"
  },
  "mpesa": {
    "consumerKey": "MPESA_CONSUMER_KEY",
    "consumerSecret": "MPESA_CONSUMER_SECRET",
    "callbackBaseUrl": "MPESA_CALLBACK_BASE"
  },
  "rave": {
    "encryptionKey": "RAVEPAY_ENCRYPTION_KEY",
    "publicKey": "RAVEPAY_PUBLIC_KEY"
  },
  "datadog": {
    "enabled": "DATADOG_ENABLED",
    "apiKey": "DATADOG_API_KEY",
    "service": "DATADOG_SERVICE",
    "source": "DATADOG_SOURCE",
    "host": "DATADOG_HOST"
  },
  "mailgun": {
    "enabled": "MAILGUN_ENABLED",
    "domain": "MAILGUN_DOMAIN",
    "apiKey": "MAILGUN_API_KEY",
    "from": "MAILGUN_FROM"
  }
}
